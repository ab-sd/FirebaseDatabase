<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- rounded colored background (36x36 to match your clock boxed style) -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFD54F" />
            <size android:width="36dp" android:height="36dp"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>

    <!-- place the calendar vector centered with 6dp insets (same visual spacing as ic_clock boxed) -->
    <item android:top="6dp" android:left="6dp" android:right="6dp" android:bottom="6dp"
        android:drawable="@drawable/ic_calendar"/>
</layer-list>
